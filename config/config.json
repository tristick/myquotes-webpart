{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "myquotes-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myquotesWebpart/MyquotesWebpartWebPart.js",
          "manifest": "./src/webparts/myquotesWebpart/MyquotesWebpartWebPart.manifest.json"
        }
      ]
    },
    "mypanel-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/mypanel/MypanelApplicationCustomizer.js",
          "manifest": "./src/extensions/mypanel/MypanelApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "MyquotesWebpartWebPartStrings": "lib/webparts/myquotesWebpart/loc/{locale}.js",
    "MypanelApplicationCustomizerStrings": "lib/extensions/mypanel/loc/{locale}.js"
  }
}
